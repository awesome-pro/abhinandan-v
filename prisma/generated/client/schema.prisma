generator client {
  provider = "prisma-client-js"
  output   = "./generated/client"
}

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}

model Patient {
  id      Int     @id @default(autoincrement())
  name    String?
  email   String
  phone   String?
  age     Int?
  address String?

  createdAt DateTime @default(now())
  updatedAt DateTime @updatedAt
}
